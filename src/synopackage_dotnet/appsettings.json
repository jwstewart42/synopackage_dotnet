{
  "AppSettings": {
    "ProxyUrl": "",
    "DefaultModel": "DS218Play",
    "DefaultVersion": "6.2.1-23824",
    "FrontendCacheFolder": "wwwroot/cache/",
    "BackendCacheFolder": "cache/",
    "CacheIconExpirationInDays": null,
    "CacheSpkServerResponse": true,
    "CacheSpkServerResponseTimeInHours": 1,
    "DownloadTimeoutInSeconds": 15
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.RollingFile"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithProcessId",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "../Logs/synopackage-{Date}.log",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] ({ProcessId}:{ThreadId:D2}) {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  }
}