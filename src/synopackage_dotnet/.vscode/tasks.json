{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/synopackage_dotnet.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "type": "npm",
            "script": "build",
            "problemMatcher": []
        },
        {
            "type": "typescript",
            "tsconfig": "tsconfig.app.json",
            "problemMatcher": [
                "$tsc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}